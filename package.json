{
  "name": "student-engagement-predictor",
  "version": "1.0.0",
  "description": "ML-based Student Engagement Prediction System for Higher Education",
  "main": "app.py",
  "scripts": {
    "setup": "python setup_complete.py",
    "train": "python train_model.py",
    "dev": "python app.py",
    "build": "cd shiksha-pulse-main && npm run build:flask",
    "install:frontend": "cd shiksha-pulse-main && npm install",
    "install:backend": "pip install -r requirements.txt",
    "install:all": "npm run install:backend && npm run install:frontend",
    "clean": "rm -rf dist && rm -rf shiksha-pulse-main/node_modules"
  },
  "dependencies": {
    "pandas": "^2.0.0",
    "numpy": "^1.24.0",
    "scikit-learn": "^1.3.0",
    "xgboost": "^1.7.0",
    "flask": "^2.3.0",
    "flask-cors": "^4.0.0",
    "joblib": "^1.3.0",
    "shap": "^0.42.0",
    "matplotlib": "^3.7.0",
    "seaborn": "^0.12.0",
    "openpyxl": "^3.1.0"
  },
  "devDependencies": {
    "jupyter": "^1.0.0",
    "notebook": "^6.5.0"
  }
}
